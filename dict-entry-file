              slot.targeting.adler_a = adlerValues[slot.ad_uuid].adler_a;
              slot.targeting.adler_b = adlerValues[slot.ad_uuid].adler_b;
              slot.targeting.adler_status = 'ok';
            }
          });
        }
      }

