        detectedDevice = 'MobileWeb';
      }

      var setInstrumentationKV = function(status, error) {
        slotConfigs.forEach(function(slot) {
          slot.targeting.adler_status = status;
          if (error) {
            slot.targeting.adler_error = error;
          }
        });
