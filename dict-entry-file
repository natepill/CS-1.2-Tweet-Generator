      }

      var fallback = function(shouldSetupDFP, event) {
        setInstrumentationKV(event.type);
        if (shouldSetupDFP) {
          setupDFP();
        }
      }

      var injectAdlerValues = function(localSlotConfigs) {
        if(adlerValues) {
