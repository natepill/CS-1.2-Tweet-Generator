        try {
          adlerValues = JSON.parse(rtfAjaxCall.responseText);
        } catch(err) {
          setInstrumentationKV('parse', err.message);
        }
        injectAdlerValues(localSlotConfigs);
      }

      var rtfCallback = function(rtfAjaxCall, localSlotConfigs, shouldSetupDFP) {
        setAdlerValues(rtfAjaxCall, localSlotConfigs);
        if (shouldSetupDFP) {
          setupDFP();
        }
      }

      var callRTF = function(localSlotConfigs, shouldSetupDFP) {
        var rtfReq = new XMLHttpRequest();
        rtfReq.addEventListener("load", rtfCallback.bind(null, rtfReq, localSlotConfigs, shouldSetupDFP));
        rtfReq.addEventListener("timeout", fallback.bind(null, shouldSetupDFP));
